#include <iostream>
using namespace std;

int main()
{
	int a,b,c;
	cout<<"Enter the three arms of the triangle:"<<endl;
	cin>> a >>b >>c;

		if ((a+b)>c){
			if((b+c)>a){
				if((c+a)>b)
	{
		cout<< " This is a valid triangle " <<endl;
		}
		else 
		{
		cout<<" This is an invalid triangle" <<endl;
		}
	}
	}	

	return 0;
}
